[user]
	name = NAME
	email = EMAIL
[core]
	excludesfile = "/Users/USERNAME/.gitignore"
	filemode = false
[color]
	interactive = auto
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = cyan
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = cyan bold
[color "status"]
	added = yellow
	changed = magenta
	untracked = cyan	
[merge]
	log = true
[alias]
	st = status
	ci = commit
	co = checkout
	df = diff
	lg = log -p
